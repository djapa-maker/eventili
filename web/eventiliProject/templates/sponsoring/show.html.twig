<!DOCTYPE html>

<title>Sponsoring</title>

{% block body %}
    <h1>Sponsoring</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id_sponso</th>
                <td>{{ sponsoring.idSponso }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ sponsoring.dateDebut ? sponsoring.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ sponsoring.dateFin ? sponsoring.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Nombre_impression</th>
                <td>{{ sponsoring.nombreImpression }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_sponsoring_index') }}">back to list</a>

    <a href="{{ path('app_sponsoring_edit', {'id_sponso': sponsoring.id_sponso}) }}">edit</a>

    {{ include('sponsoring/_delete_form.html.twig') }}
{% endblock %}
