<!DOCTYPE html>

<title>Imgev index</title>

{% block body %}
    <h1>Imgev index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdImgev</th>
                <th>Imagee</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for imgev in imgevs %}
            <tr>
                <td>{{ imgev.idImgev }}</td>
                <td>{{ imgev.imagee }}</td>
                <td>
                    <a href="{{ path('app_imgev_show', {'idImgev': imgev.idImgev}) }}">show</a>
                    <a href="{{ path('app_imgev_edit', {'idImgev': imgev.idImgev}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_imgev_new') }}">Create new</a>
{% endblock %}
